<div class="panel-heading">
  <div class="search">
    <input type="text" placeholder="search by Author Name">
  </div>
  {{#link-to "author.create" class="btn-new"}}
  <div class="sr-only">Add New Author</div>
  {{/link-to}}
</div>

<div class="panel-content">
  <h3 class="panel-title">Authors</h3>
  <ul class="collection">
    {{#each model as |author|}}
    {{author-list-item author=author tagName="li"}}
    {{/each}}
  </ul>
</div>
{{outlet}}